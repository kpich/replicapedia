
<%= form_for :paper, url: papers_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :conference%><br>
    <%= f.text_field :conference%>
  </p>

  <p>
    <%= f.label :year %><br>
    <%= f.number_field :year %>

  <p>
  <%= f.submit %>
  </p>
<% end %>
